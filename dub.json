{
	"name": "goinsu",
	"authors": [
		"Anton Fediushin"
	],
	"description": "Switch user, group and execute a program",
	"copyright": "Copyright Â© 2017, Anton Fediushin",
	"license": "MIT",
	"mainSourceFile": "goinsu.d",
	"importPaths": ["."],
	"configurations": [
		{
			"name": "executable",
			"targetType": "executable",
			"dflags": ["-betterC"]
		},
		{
			"name": "static",
			"targetType": "executable",
			"dflags": ["-betterC"],
			"dflags-ldc": ["-betterC", "-static"],
			"lflags": ["-static"]
		}
	],
	"preGenerateCommands": [
		"bash -c \"echo 'module version_; enum VERSION=\\\"$$(git -C . describe --tags)\\\";' > version_.d\""
	]
}
